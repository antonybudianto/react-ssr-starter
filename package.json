{
  "name": "react-ssr-starter",
  "version": "1.0.0",
  "description": "react ssr starter",
  "main": "index.js",
  "scripts": {
    "start": "better-npm-run start",
    "clean": "rimraf dist",
    "lint": "eslint config src",
    "test": "jest",
    "deploy": "better-npm-run deploy",
    "dev:server": "better-npm-run dev:server",
    "build:client": "better-npm-run build:client",
    "build:server": "better-npm-run build:server",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:staging": "better-npm-run deploy:staging",
    "deploy:prod": "better-npm-run deploy:prod",
    "start:dev": "node dist/bundle.js",
    "prettier": "prettier --write \"{src,config}/**/*.js\""
  },
  "betterScripts": {
    "start": {
      "command": "npm run clean && npm run dev:server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "dev:server": {
      "command": "webpack --config ./config/webpack.server.config.babel.js --hot --watch"
    },
    "build:server": {
      "command": "webpack --config ./config/webpack.server.config.babel.js --progress"
    },
    "build:client": {
      "command": "webpack --config ./config/webpack.client.config.babel.js --progress"
    },
    "deploy": {
      "command": "npm run clean && npm-run-all build:*"
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "deploy:staging": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "staging"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "author": "Antony",
  "license": "MIT",
  "jest": {
    "globals": {
      "__DEV__": false,
      "__TEST__": true,
      "__PROD__": false,
      "__STAG__": false
    }
  },
  "dependencies": {
    "axios": "0.18.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.4",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "better-npm-run": "^0.1.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.11",
    "debug": "^3.1.0",
    "dotenv": "5.0.1",
    "express": "4.16.3",
    "file-loader": "^1.1.11",
    "loadable-components": "2.2.2",
    "lodash": "4.17.10",
    "lodash-es": "4.17.10",
    "mini-css-extract-plugin": "0.4.0",
    "morgan": "1.9.0",
    "node-sass": "^4.9.0",
    "npm-run-all": "4.1.3",
    "null-loader": "0.1.1",
    "optimize-css-assets-webpack-plugin": "^4.0.1",
    "prop-types": "^15.6.1",
    "query-string": "^6.1.0",
    "raf": "3.4.0",
    "react": "16.4.0",
    "react-body-classname": "^1.2.0",
    "react-dom": "16.4.0",
    "react-helmet": "5.2.0",
    "react-hot-loader": "^4.2.0",
    "react-redux": "5.0.7",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "recompose": "0.27.1",
    "redux": "4.0.0",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "serialize-javascript": "1.5.0",
    "style-loader": "^0.21.0",
    "ua-parser-js": "0.7.18",
    "url-loader": "^1.0.1",
    "webpack": "4.8.3",
    "webpack-cli": "^2.1.4",
    "webpack-merge": "4.1.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-jest": "^21.15.2",
    "eslint-plugin-react": "^7.8.2",
    "http-proxy-middleware": "^0.18.0",
    "jest": "^22.4.4",
    "prettier": "1.12.1",
    "start-server-webpack-plugin": "^3.0.0-rc3",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-manifest-plugin": "^2.0.3",
    "webpack-node-externals": "^1.7.2"
  }
}
